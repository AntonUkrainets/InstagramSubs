<?xml version="1.0" encoding="utf-8" ?>
<Shell 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:prism="http://prismlibrary.com"
    xmlns:stores="clr-namespace:InstagramSubs.Views.Stores"
    prism:ViewModelLocator.AutowireViewModel="True"
    x:Class="InstagramSubs.Views.StoreView"
    Shell.NavBarIsVisible="True">

    <Tab>
        <ShellContent Title="Buy Coins">
            <stores:BuyStoreView/>
        </ShellContent>
        <ShellContent Title="Free Coins">
            <stores:FreeStoreView/>
        </ShellContent>
    </Tab>

</Shell>